<% thumb = if @presenter.id == "fk47w6q89d"
  ['newspapers-collection.png']
else
  random_thumbnail_from_collection(@member_docs)
end %>

<%=
  link_to collection_path(@presenter), data: { turbolinks: false } do
    if thumb
      image_tag thumb.first, alt: @presenter.title, class: 'collection-representative'
    else
      content_tag(:span, '', class: 'glyphicon glyphicon-th collection-icon')
    end
  end
%>
